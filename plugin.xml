<?xml version="1.0" encoding="UTF-8"?>
<plugin id="app-entitlements" version="0.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0">
  <name>AppEntitlements</name>
  <platform name="ios">
    <config-file platform="ios" target="Entitlements-*.plist" parent="com.apple.developer.associated-domains" mode="merge">
     <array>
         <dict>
              <key>applinks</key>
              <string>XXXXX.com</string>
        </dict>
     </array>
    </config-file>
  </platform>
</plugin>
